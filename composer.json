{
    "name": "whispphp/whisp",
    "description": "Pure PHP SSH server designed for terminal apps",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Whisp\\": "src/"
        }
    },
    "scripts": {
        "format": "vendor/bin/pint",
        "test": "vendor/bin/pest"
    },
    "authors": [
        {
            "name": "Ashley Hindle",
            "email": "hey@ashleyhindle.com"
        }
    ],
    "require": {
        "php": "^8.2|^8.3|^8.4",
        "ext-pcntl": "*",
        "ext-sodium": "*",
        "ext-ffi": "*",
        "ext-mbstring": "*",
        "psr/log": "^3.0",
        "phpseclib/phpseclib": "^3.0"
    },
    "require-dev": {
        "laravel/pint": "^1.21",
        "pestphp/pest": "^3.7"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
